default-pattern : U32 -> String;
default-pattern ignored = case ignored {
    _ => "default";
};

duplicate-patterns : U32 -> String;
duplicate-patterns ignored = case ignored {
    42 => "forty-two";
    42 => "42";
    _ => "default";
};

meaning : U32 -> String;
meaning magic = case magic {
    7 => "the common choice";
    42 => "life, the universe, and everything";
    13 => "unlucky";
    _ => "nonsense";
};

hi-to-bye : String -> String;
hi-to-bye value = case value {
    "hi" => "bye";
    other => other;
};

