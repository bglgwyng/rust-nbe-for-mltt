meaning-1 : U32 -> String;
meaning-1 magic = case magic {
    7 => "the common choice";
    42 => "life, the universe, and everything";
    13 => "unlucky";
    _ => "nonsense";
};

meaning-2 : U32 -> String;
meaning-2 = case {
    7 => "the common choice";
    42 => "life, the universe, and everything";
    13 => "unlucky";
    _ => "nonsense";
};

hi-to-bye : String -> String;
hi-to-bye = case {
    "hi" => "bye";
    other => other;
};

both-zero-1 : U32 -> Char -> String;
both-zero-1 n1 n2 = case n1 n2 {
    0 '0' => "yup";
    0 _   => "almost";
    _ '0' => "possibly";
    _ _   => "nope";
};

both-zero-2 : U32 -> Char -> String;
both-zero-2 n = case n {
    0 '0' => "yup";
    0 _   => "almost";
    _ '0' => "possibly";
    _ _   => "nope";
};

both-zero-3 : U32 -> Char -> String;
both-zero-3 = case {
    0 '0' => "yup";
    0 _   => "almost";
    _ '0' => "possibly";
    _ _   => "nope";
};
